context ctx
{
	i: int
	j: int
	k: int

	lex start
	{
		ignore /space+/
		literal '*', '(', ')'
		token id /[a-zA-Z_]+/ 
	}

	def foo [id]

	int f()
	{
		i = i + 1
	}

	def item 
		[id]
	|	[foo]
	|	['(' item* ')']
		{
			i = 0
			f()
			f()
			f()
			print( i, '\n' )
		}
	

	def start 
		[item*]
}

CTX: ctx = cons ctx []
Input: ctx::start = parse ctx::start( CTX, stdin )
print( Input, '\n' )


