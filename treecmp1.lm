rl ident_pattern /[a-zA-Z_][a-zA-Z_0-9]*/
rl number_pattern /[0-9]+/

lex start
{
	ignore /[ \t\n]+/
	token id /ident_pattern/
	token number /number_pattern/
}

def four_ids
	[id id id id]

id B = construct id "b"
four_ids Input = parse four_ids( stdin )

for Id: id in Input {
	if ( Id == B )
		print( B, '\n' )
}
