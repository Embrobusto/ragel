lex start
{
	ignore /space+/
	literal '*', '(', ')'
	token id /[a-zA-Z_]+/
}

def item 
	[id]
|	['(' item* ')']

def start 
	[item*]

parse InputP: start[ stdin ]
Input: start = InputP.tree

cons Output: accum<start> []

for Id: id in Input {
	send Output
		"( [^Id] )
}

S: start = Output()

print( S '\n' )
