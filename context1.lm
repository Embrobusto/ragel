context ctx
{
	i: int
	j: int
	k: int

	lex start
	{
		ignore /space+/
		literal '*', '(', ')'
		token id /[a-zA-Z_]+/ 
	}

	def foo [id]

	def item 
		[id]
	|	[foo]
	|	['(' item* ')']
		{
			i = 0
			j = i + 1
			k = j + 1
			print( k '\n' )
		}

	def start 
		[item*]
}

CTX: ctx = cons ctx[]
Input: ctx::start = parse ctx::start( CTX stdin )
print( Input '\n' )
