
lex start
{
    literal '{', '}'
    literal 'struct', 'type'
    token id /[A-Za-z_][A-Za-z_0-9]*/

    ignore /'/*' any* :>> '*/'/
    ignore /[ \t\r\n]+/
}

def attribute
    ['type' id]

def struct
    ['struct' id '{' attribute* '}']

def program
    [struct*]

int func( P: program )
{
}

int main()
{
    InputFile: stream = open( 'func.in', "r" )
    P: program = parse program( InputFile )
	func( P )
	print( P )
}

main()

