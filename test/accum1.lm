lex
{
	ignore /space+/
	literal '*', '(', ')'
	token id /[a-zA-Z_]+/
}

def item 
	[id]
|	['(' item* ')']

def start 
	[item*]

parse Input: start[ stdin ]
InputTree: start = Input.tree

cons Output: accum<start> []

for Id: id in InputTree {
	send Output
		"( [^Id] )
}

S: start = Output()

print( S '\n' )
